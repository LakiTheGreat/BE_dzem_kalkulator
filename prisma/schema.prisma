

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Fruit {
  id Int @id @default(autoincrement())
  value String 
  menuItemLabel String
  isDeleted Boolean @default(false)
}

model Cup {
  id Int @id @default(autoincrement())
  value Int 
  menuItemLabel String
  isDeleted Boolean @default(false)
}

